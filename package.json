{
  "name": "bot-app",
  "version": "0.1.0",
  "private": true,
  "description": "Bot App, aplication for derivery online to whatsapp",
  "author": {
    "name": "Danilo Pinto"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "build": {
    "appId": "com.bot-app",
    "productName": "Bot App",
    "copyright": "Copyright Â© 2019 Danilo",
    "win": {
      "icon": "./images/bot.ico",
      "target": {
        "target": "dir",
        "arch": ["x64"]
      }
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "electron-is-dev": "^1.2.0",
    "frameless-titlebar": "^2.1.3",
    "qrcode": "^1.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "whatsapp-web.js": "^1.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron public/electron.js\"",
    "dev:app": "electron public/electron.js",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-build": "yarn build",
    "electron-build": "electron-builder -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.14",
    "@rescripts/rescript-env": "^0.0.12",
    "concurrently": "^5.2.0",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "typescript": "^3.9.6",
    "wait-on": "^5.1.0"
  }
}
